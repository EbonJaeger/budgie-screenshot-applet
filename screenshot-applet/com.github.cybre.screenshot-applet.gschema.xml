<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="budgie">
  <schema id="com.github.cybre.screenshot-applet">

    <key type="b" name="enable-label">
        <default>false</default>
    </key>

    <key type="b" name="enable-local">
        <default>false</default>
    </key>

    <key type="s" name="provider">
        <default>'imgur'</default>
    </key>

    <key type="b" name="use-main-display">
        <default>true</default>
    </key>

    <key type="i" name="monitor-to-use">
        <default>0</default>
    </key>

    <key type="i" name="delay">
        <range min="0" max="60"/>
        <default>3</default>
    </key>

    <key type="b" name="include-border">
        <default>true</default>
    </key>

    <key type="s" name="window-effect">
        <default>'none'</default>
    </key>

    <key type="a(xss)" name="history">
        <default>[]</default>
    </key>

  </schema>
</schemalist>